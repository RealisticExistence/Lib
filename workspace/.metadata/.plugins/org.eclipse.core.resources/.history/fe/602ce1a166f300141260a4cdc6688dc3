package main;

import net.lib.frame.window.Content;
import net.lib.frame.window.Window;



public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Window w = new Window();
		w.enableThis(true);
		Content c = new Content(w);
		c.drawText("hola", 100, 100);
		w.addContent(c);
	}

}

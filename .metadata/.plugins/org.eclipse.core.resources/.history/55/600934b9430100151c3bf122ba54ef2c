package main;


import net.tpdl.lib.content.Content;
import net.tpdl.lib.frame.Img;
import net.tpdl.lib.frame.Window;



public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	Window w = new Window();
	w.enableThis(true);
	Content c = new Content(w);
	w.addContent(c);
	c.drawText("Hola", 1000, 1000);
	c.drawText("Halo", 1000, 1000);
	c.moveText("Hola", 100, 100, 100);
	c.moveText("Halo", 1300, 100, 100);
	
	//c.getVectorText("Hola");


	}

}
